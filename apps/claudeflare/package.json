{
  "name": "claudeflare",
  "version": "0.0.3",
  "description": "A simple CLI tool that outputs Hello World",
  "main": "dist/app.js",
  "type": "module",
  "bin": {
    "claudeflare": "./bin/claudeflare.js"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "build:web": "tsc --project tsconfig.web.json",
    "build:all": "pnpm build && pnpm build:web",
    "start": "node ./bin/claudeflare.js",
    "dev": "pnpm build:all && node ./bin/claudeflare.js",
    "prepublishOnly": "pnpm build:all"
  },
  "files": [
    "dist/**/*",
    "bin/**/*",
    "README.md",
    "package.json"
  ],
  "keywords": [
    "cli",
    "hello-world"
  ],
  "author": "hmmhmmhm",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hmmhmmhm/claudeflare.git"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^22.15.3",
    "@types/react": "^19.1.8",
    "eslint": "^9.31.0",
    "react": "^19.1.0",
    "typescript": "5.8.2"
  },
  "dependencies": {
    "@hono/node-server": "^1.18.2",
    "hono": "^4.9.2",
    "ink": "^6.0.1"
  }
}
